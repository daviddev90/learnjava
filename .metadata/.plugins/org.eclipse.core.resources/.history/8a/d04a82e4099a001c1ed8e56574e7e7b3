package com.javalec.function;

public class ConstDan {

	int dan = 0;

	public ConstDan(int dan) {
		super();
		this.dan = dan;
	}
	
	CheckEvenAndReturn checkReturn = new CheckEvenAndReturn();
	
	public void printDan() {
		for(int i = 1; i <= 9; i++) {
			System.out.println("const) " + checkReturn.ifEvenReturnStar(i, dan));
		}
	}
	
	public String[] returnArrDan() {
		String[] arr = new String[9];
		
		for(int i = 1; i <= 9; i++) {
			arr[i-1] = "constArray) " + checkReturn.ifEvenReturnStar(i, dan);
		}
		
		return arr;
	}
	
	
}
