package com.javalec.function;

public class FieldDan {
	public int dan = 0;
	
	public FieldDan() {
		
	}
	CheckEvenAndReturn checkReturn = new CheckEvenAndReturn();
	
	public void printDan() {
		for(int i = 1; i <= 9; i++) {
			System.out.println("(Field) " + checkReturn.ifEvenReturnStar(i, dan));
		}
	}
	
	public String[] returnArrDan() {
		String[] arr = new String[9];
		
		for(int i = 1; i <= 9; i++) {
			arr[i-1] = ("(FieldArr) " + checkReturn.ifEvenReturnStar(i, dan));
		}
		
		return arr;
	}
}
