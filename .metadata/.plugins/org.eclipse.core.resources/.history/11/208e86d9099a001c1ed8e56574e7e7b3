package com.javalec.function;

public class FieldDan {
	public int dan = 0;
	
	public FieldDan() {
		
	}
	
	public void printDan() {
		for(int i = 1; i <= 9; i++) {
			System.out.println("(Field) " + dan + " X " + i + " = " + (dan * i));
		}
	}
	
	public String[] returnArrDan() {
		String[] arr = new String[9];
		
		for(int i = 1; i <= 9; i++) {
			arr[i-1] = ("(FieldArr) " + dan + " X " + i + " = " + (dan * i));
		}
		
		return arr;
	}
}
