package com.javalec.base;

public class Calc {

	//main > ctrl + space > enter
	public static void main(String[] args) {
		
		//변수를 이용한 사칙연
		int intNum1 = 10;
		int intNum2 = 20;
		
		System.out.println(intNum1 + intNum2);
		System.out.println(intNum1 - intNum2);
		System.out.println(intNum1 * intNum2);
		System.out.println(intNum1 / intNum2);
		
		
		
		// 상수를 이용한 사칙연
		System.out.println(10*20); //200
		System.out.println(20/10); //2
		System.out.println(10/20); //0
	}

}

