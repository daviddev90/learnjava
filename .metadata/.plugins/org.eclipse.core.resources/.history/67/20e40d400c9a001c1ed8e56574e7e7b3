package com.javalec.function;

public class MethodDan {
	
	public MethodDan() {
		
	}
	ReturnDanString returnDanString = new ReturnDanString();
	Config config = new Config();

	public void printDan(int dan) {
		for(int i = 1; i <= config.maxMultiple; i++) {
			System.out.println("(Method) " + returnDanString.returnString(i, dan));
		}
	}
	
	public String[] returnArrDan(int dan) {
		String[] arr = new String[9];
		
		for(int i = 1; i <= config.maxMultiple; i++) {
			arr[i-1] = ("(MethodArr) " + returnDanString.returnString(i, dan));
		}
		
		return arr;
	}
}
