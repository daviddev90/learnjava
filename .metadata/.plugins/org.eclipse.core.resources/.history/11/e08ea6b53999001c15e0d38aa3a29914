package com.javalec.base;


public class Main {

	public static void main(String[] args) {
		int[] arr = new int[10000];
		
		int result = 0;
		int maker = 1;
		
		while(result < 10000) {
			int n = maker;
			result = maker;
			
			while(n>0) {
				result += n % 10;
				n = n/10;
			}
			if(result <= 10000) {
				arr[result]++;
			}
			maker++;
		}
		
		System.out.println(arr.toString());
	}
}
