package com.javalec.function;

public class ConstCalc {
	int num1;
	int num2;
	
	private void constCac() {
		// TODO Auto-generated method stub

	}

	public ConstCalc(int num1, int num2) {
		super();
		this.num1 = num1;
		this.num2 = num2;
	}
	
	public String add() {
		return (num1 + " + " + num2 + " = " + (num1 + num2));
	}
}
