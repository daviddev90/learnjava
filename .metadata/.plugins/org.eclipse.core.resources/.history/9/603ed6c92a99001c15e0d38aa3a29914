package com.javalec.base;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int a = 1, b = 1;
		
		while(((a!=0) || (b!=0)) && in.hasNextInt()) {
			a = in.nextInt();
			b = in.nextInt();
			if(a!=0||b!=0)	{
                System.out.println(a + b);
            }
		}
		
	}
}
