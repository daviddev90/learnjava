package com.javalec.base;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int max = 0;
		int maxIdx = 0;
		
		for (int i = 1; i < 9; i++) {
			int n = in.nextInt(); 
			if(n > max) {
				max = n;
				maxIdx = n + 1;
			}
		}
		
		System.out.println(max);
		System.out.println(maxIdx);
	}
	
}
