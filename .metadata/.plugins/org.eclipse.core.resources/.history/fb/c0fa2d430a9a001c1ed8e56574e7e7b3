package com.javalec.function;

public class MethodDan {
	
	public MethodDan() {
		
	}
	CheckEvenAndReturn checkReturn = new CheckEvenAndReturn();

	public void printDan(int dan) {
		for(int i = 1; i <= 9; i++) {
			System.out.println("(Method) " + checkReturn.returnString(i, dan));
		}
	}
	
	public String[] returnArrDan(int dan) {
		String[] arr = new String[9];
		
		for(int i = 1; i <= 9; i++) {
			arr[i-1] = ("(MethodArr) " + checkReturn.returnString(i, dan));
		}
		
		return arr;
	}
}
