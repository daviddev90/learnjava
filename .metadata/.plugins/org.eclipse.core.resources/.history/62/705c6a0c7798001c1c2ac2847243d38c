package com.javalec.base;

public class CalcSum {
	//Field (Property)
	
	//Constructor
	public CalcSum() {
		
	}
	
	//Method
	//합계를 구하는 Method
	public int sum(int startNum, int endNum) {
		int sum = 0;
		for (int i = startNum; i<=endNum; i++) {
			sum += i;
		}
		return sum;
	}
	
	//홀짝을 판단하는 Method
	
}
