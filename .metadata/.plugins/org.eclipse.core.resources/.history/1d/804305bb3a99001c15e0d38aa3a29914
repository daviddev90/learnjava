package com.javalec.base;


public class Main {

	public static void main(String[] args) {
		int[] arr = new int[10000];
		
		int result = 0;
		int maker = 1;
		
		while(result < 10000) {
			int n = maker;
			result = maker;
			
			while(n>0) {
				result += n % 10;
				n = n/10;
			}
			if(result <= 9999) {
				arr[result]++;
			}
			maker++;
		}
		
		for(int i = 0; i < 10000; i++) {
			if(arr[i] == 0) {
				System.out.println(i);
			}
		}
	}
}
